{
    "name": "sculpin/blog-skeleton",
    "description": "A Skeleton for a Sculpin Based Blog",
    "type": "sculpin-skeleton",
    "license": "MIT",
    "authors": [
        {
            "name": "Dragonfly Development Inc.",
            "email": "info@dflydev.com",
            "homepage": "http://dflydev.com"
        },
        {
            "name": "Beau Simensen",
            "email": "beau@dflydev.com",
            "homepage": "http://beausimensen.com"
        }
    ],
    "require": {
        "knplabs/github-api": "1.0.*@DEV",
        "sculpin/sculpin": "2.0.*@DEV",
        "composer/composer": "1.0.*@DEV",
        "dflydev/embedded-composer-console": "1.0.*@DEV",
        "dflydev/embedded-composer-core": "1.0.*@DEV",
        "sculpin/sculpin-theme-composer-plugin": "1.0.*@DEV",
        "components/bootstrap": "~2.3.1",
        "components/jquery": "~1.9.1",
        "components/highlightjs": "~7.3.0"
    },
    "require-dev": {
        "phpunit/phpunit": "~4.0"
    },
    "config": {
        "component-dir": "source/components"
